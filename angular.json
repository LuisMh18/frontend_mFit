{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mFit": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mFit",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/theme-default.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/app/assets/js/plugins/jquery/jquery.min.js",
              "src/app/assets/js/plugins/jquery/jquery-ui.min.js",
              "src/app/assets/js/plugins/bootstrap/bootstrap.min.js",
              "src/app/assets/js/plugins/icheck/icheck.min.js",
              "src/app/assets/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js",
              "src/app/assets/js/plugins/scrolltotop/scrolltopcontrol.js",
              "src/app/assets/js/plugins/morris/morris.min.js",
              "src/app/assets/js/plugins/rickshaw/d3.v3.js",
              "src/app/assets/js/plugins/rickshaw/rickshaw.min.js",
              "src/app/assets/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
              "src/app/assets/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
              "src/app/assets/js/plugins/bootstrap/bootstrap-datepicker.js",
              "src/app/assets/js/plugins/owl/owl.carousel.min.js",
              "src/app/assets/js/plugins/moment.min.js",
              "src/app/assets/js/plugins/daterangepicker/daterangepicker.js",
              "src/app/assets/js/settings.js",
              "src/app/assets/js/plugins.js",
              "src/app/assets/js/actions.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mFit:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mFit:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mFit:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/js/custom.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mFit:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mFit:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "mFit"
}